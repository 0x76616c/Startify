<Window x:Name="startmenucompactbasewindow" x:Class="WPF.Views.StartMenuCompact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:xamlhost="clr-namespace:Microsoft.Toolkit.Wpf.UI.XamlHost;assembly=Microsoft.Toolkit.Wpf.UI.XamlHost"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Views"
        mc:Ignorable="d"
        Title="StartMenuCompact"        
        WindowStyle="None"
        AllowsTransparency="True"
        Background="Transparent"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        Topmost="True"
        Height="500"
        Width="260"
        Visibility="Hidden"  Activated="StartMenuActivated" Deactivated="StartMenuDeactivated">
    <Grid>
        <xamlhost:WindowsXamlHost x:Name="StartMenuIslandhCompact" InitialTypeName="Shell.Interface.StartMenuCompact.StartMenuCompact"/>
    </Grid>
</Window>
