<ContentDialog
    x:Class="Shell.Interface.StartMenu11.Controls.UninstallFlyoutControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Shell.Interface.StartMenu11.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="650"
    Style="{StaticResource DefaultContentDialogStyle}"
    d:DesignWidth="650"
    Title="Uninstall APP"
    SecondaryButtonText="Cancel"
    PrimaryButtonText="Uninstall"
    SecondaryButtonStyle="{StaticResource AccentButtonStyle}">
    <Grid>
        <TextBlock Text="This app and its related information will be removed." VerticalAlignment="Center" TextWrapping="Wrap" Style="{StaticResource BodyTextBlockStyle}" />
    </Grid>
</ContentDialog>
